{
    "name": "Prep New Tab Page",
    "manifest_version": 2,
    "description": "Replaces the awful new tab page",
    "version": "0.0.0.1",
    "permissions" : [
      "management",
      "https://google.com/search?q=*",
      "tabs"],

    "chrome_url_overrides" : {
        "newtab": "./index.html"
    },
    "csp": "script-src 'self' 'unsafe-inline'; object-src 'self'"

 }
